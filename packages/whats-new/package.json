{
  "name": "@nextdevx/whats-new",
  "version": "0.2.0",
  "description": "What's new / changelog component with voting and feedback linking",
  "license": "MIT",
  "author": "Dylan van Iersel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/applab-nl/next-components.git",
    "directory": "packages/whats-new"
  },
  "homepage": "https://github.com/applab-nl/next-components/tree/main/packages/whats-new#readme",
  "bugs": {
    "url": "https://github.com/applab-nl/next-components/issues"
  },
  "keywords": [
    "nextjs",
    "react",
    "changelog",
    "whats-new",
    "release-notes",
    "voting"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./admin": {
      "types": "./dist/admin/index.d.ts",
      "import": "./dist/admin/index.js"
    }
  },
  "files": [
    "dist",
    "prisma"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@nextdevx/core": "workspace:*"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0",
    "lucide-react": ">=0.300.0",
    "next": ">=14.0.0",
    "next-intl": ">=3.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "next-intl": {
      "optional": true
    }
  },
  "devDependencies": {
    "@prisma/client": "^6.2.1",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "lucide-react": "^0.469.0",
    "next": "^15.1.4",
    "next-intl": "^3.26.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.3"
  }
}
