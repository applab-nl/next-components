{
  "name": "@nextdevx/feedback",
  "version": "0.2.0",
  "description": "User feedback system with element picker, screenshots, and issue tracker integration",
  "license": "MIT",
  "author": "Dylan van Iersel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/applab-nl/next-components.git",
    "directory": "packages/feedback"
  },
  "homepage": "https://github.com/applab-nl/next-components/tree/main/packages/feedback#readme",
  "bugs": {
    "url": "https://github.com/applab-nl/next-components/issues"
  },
  "keywords": [
    "nextjs",
    "react",
    "feedback",
    "screenshot",
    "bug-report",
    "user-feedback"
  ],
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./admin": {
      "types": "./dist/admin/index.d.ts",
      "import": "./dist/admin/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js"
    }
  },
  "files": [
    "dist",
    "prisma"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@nextdevx/core": "workspace:*"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0",
    "lucide-react": ">=0.300.0",
    "modern-screenshot": ">=4.0.0",
    "next": ">=14.0.0",
    "next-intl": ">=3.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "next-intl": {
      "optional": true
    },
    "modern-screenshot": {
      "optional": true
    }
  },
  "devDependencies": {
    "@prisma/client": "^6.2.1",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "lucide-react": "^0.469.0",
    "modern-screenshot": "^4.4.39",
    "next": "^15.1.4",
    "next-intl": "^3.26.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.3"
  }
}
