  "dependencies": {
    "commander": "^12.1.0",
    "prompts": "^2.4.2",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/prompts": "^2.4.9",
    "typescript": "^5.7.3"
  }
